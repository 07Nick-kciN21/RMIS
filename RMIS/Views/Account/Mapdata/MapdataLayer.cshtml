

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />
    <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/select2-bootstrap-5-theme@1.3.0/dist/select2-bootstrap-5-theme.min.css" />

    <script type="module" src="~/js/account/mapdata/mapdataLayer.js"></script>
    <link rel="stylesheet" href="~/css/Account/mapdataLayer.css" />
</head>
<body>
    <div class="modal-header">
        <h5 class="modal-title">查看圖資</h5>
    </div>
    <div class="modal-content">
        <div>
            @* 三個select LayerSelect、DistSelect、NameSelect *@
            <div class="d-flex justify-content-between align-items-center container mt-4">
                <select id="mapdataLayerSelector" class="selector form-select"></select>
                <select id="mapdataDistSelector" class="selector form-select"></select>
                <select id="mapdataNameSelector" class="selector form-select"></select>
                <button id="mapdataSearch" class="btn btn-primary" style="width:100px">查詢</button>
            </div>
        </div>
        <div class="modal-body">
            <div id="layerContainer" class="container mt-4 d-none">
            </div>
        </div>
    </div>
</body>
</html>
